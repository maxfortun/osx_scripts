<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Label</key>
	<string>kidsOff</string>
	<key>ProgramArguments</key>
	<array>
		<string>${OSX_SCRIPTS}/kidsOff.sh</string>
	</array>
	<key>StartCalendarInterval</key>
	<array>
		<dict>
			<key>Hour</key>
			<integer>19</integer>
			<key>Minute</key>
			<integer>45</integer>
		</dict>
	</array>
	<key>StandardOutPath</key>
	<string>/Users/max/logs/kidsOff.out</string>
	<key>StandardErrorPath</key>
	<string>/Users/max/logs/kidsOff.err</string>
</dict>
</plist>
